{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/webhook.ts"
    }
  ],
  "functions": {
    "api/webhook.ts": {
      "maxDuration": 30
    }
  }
}